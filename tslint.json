{
  "extends": "tslint:latest",
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      "ban-keywords",
      "check-format"
    ],
    "no-implicit-dependencies": false,
    "no-var-requires": false,
    "no-submodule-imports": false
  },
  "compilerOptions": {
    "module": "commonjs"
  }
}
